#include<stdio.h>
#include<stdlib.h>


int main()
{
  long long int T, N ; 
  long long int maxFact[65]={1,     2,     3,  	4,	6,	9,	15,	24,	43,	71,	130,	222,	412,	721,	1351,	2401,	4535,	8163,	15509,	28198,	53832,	98692,	189159,	349216,	671569,	1247199,	2405312,	4489915,	8680502,	16275941,	31534635,	59359312,	115226899,	217650809,	423209906,	801871400,	1561539042,	2966924179,	5785502149,	11020004093,	21515246086,	41074560709,	80282095931,	153583140041,	300488752254,	575936775153,	1127876184674,	2165522274574,	4244423658165,	8162353188778,	16010769716449,	30835556490938,	60529055334063,	116734606715692,	229300120334395,	442786439266417,	870304380627095,	1682588469212383,	3309090656117686,	6404691592485843,	12602780230375368,	24417886696352275,	48072714433443541,	93231931022435958,	183638652013889008};


  scanf("%d",&T);
  while(T--)
    {

      scanf("%d",&N);
      

      for(i=0;i<65;i++){
	if(maxFact[i]> N ) printf("%lld\n",i);
  
      } 
 
 

    } 


  return 0;
}
