#include<stdio.h>
#include<stdlib.h>


int read()
{
	char c = 0;
int input = 0;
while(c < 33)
c = getchar_unlocked();
while(c > 33)
{
input = input*10+c-'0';
c = getchar_unlocked();
}
return input;
}



