#include <iomanip>
#include <cstdio>
#include <cmath>
#include <cstdlib>
#include <cctype>
#include <string>
#include <cstring>
#include <ctime>
#include <string.h>

using namespace std;

int main ()
{
    long long int T,i,n,j,l,r,t,temp,left,right,lidx;	
	cin>>t;
	for (i = 0; i < t; i += 1)
	{
		cin>>n>>l>>r;
		
		lidx=n/l;
		left=lidx*l;
		right=lidx*r;
			
		if(n>=left && n<=right)cout<<"Yes"<<endl;
		else cout<<"No"<<endl;
		
	}
	return 0;
}

