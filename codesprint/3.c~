#include<stdio.h>
#include<stdlib.h>

long int ans(int n)
{
  
 lon long  int num[100000];
int power;
  num[0]=9;num[1]=90;num[2]=99;
  if(num[0]%n==0)return num[0];
  else if(num[1]%n==0)return num[1];
  else if(num[2]%n==0)return num[2];
  power=2;



  
}


int main()
{
  int T,n;
  long long int number;  
  scanf("%d",&T);
  while(T--)
    {
      scanf("%d",&n);
      number=ans(n);
      printf("%lld\n",number);
  
    }

  return 0;

}
